{
  "name": "@bizon/mws-sdk",
  "version": "4.4.0",
  "main": "index.js",
  "description": "SDK for Amazon Marketplace Web Services",
  "homepage": "https://github.com/bizon/mws-sdk#readme",
  "license": "MIT",
  "keywords": [
    "amazon",
    "amazon-marketplace",
    "amazon-mws",
    "mws",
    "sellercentral"
  ],
  "scripts": {
    "test": "TZ=UTC jest"
  },
  "files": [
    "index.js",
    "lib/**/*.js"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bizon/mws-sdk.git"
  },
  "author": "Bertrand Marron <bertrand.marron@gmail.com>",
  "dependencies": {
    "@bizon/amazon-ids": "^2.3.0",
    "content-type": "^1.0.4",
    "csv-parse": "^4.14.1",
    "date-fns": "^2.16.1",
    "debug": "^4.3.1",
    "got": "^11.8.1",
    "iconv-lite": "^0.6.2",
    "libxmljs": "^0.19.7",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "codecov": "^3.8.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lerna-changelog": "^1.0.1",
    "mockdate": "^3.0.2",
    "nock": "^13.0.5",
    "xo": "^0.37.1"
  },
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/bizon/mws-sdk/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/__tests__/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__helpers__/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "reports/tests"
        }
      ]
    ],
    "globalSetup": "./jest.setup.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "index.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-reduce": "off"
    },
    "overrides": [
      {
        "files": "__tests__/**/*.js",
        "envs": [
          "jest"
        ]
      }
    ]
  }
}
