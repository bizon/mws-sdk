{
  "name": "@bizon/mws-sdk",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "TZ=UTC jest"
  },
  "files": [
    "index.js",
    "lib/**/*.js"
  ],
  "repository": "git@github.com:bizon/mws-sdk.git",
  "author": "Bertrand Marron <bertrand.marron@gmail.com>",
  "dependencies": {
    "content-type": "^1.0.4",
    "csv-parse": "^4.4.1",
    "date-fns": "^1.30.1",
    "got": "^9.0.0",
    "iconv-lite": "^0.4.24",
    "jschardet": "^2.1.0",
    "libxmljs": "^0.19.5"
  },
  "devDependencies": {
    "codecov": "^3.3.0",
    "jest": "^24.7.1",
    "jest-junit": "^6.4.0",
    "xo": "^0.24.0"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "output": "reports/tests/junit.xml"
        }
      ]
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "index.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "overrides": [
      {
        "files": "__tests__/**/*.js",
        "envs": [
          "jest"
        ]
      }
    ]
  }
}
